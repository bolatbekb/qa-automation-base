<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Backend">
    <listeners>
        <listener class-name="listeners.RetryListener" />
        <listener class-name="listeners.EventListener" />
    </listeners>
    <test name="Regression for Backend" >
        <groups>
            <run>
                <include name="api"/>
            </run>
        </groups>

        <packages>
            <package name="test.java.backend.*" />
        </packages>
    </test>
</suite>