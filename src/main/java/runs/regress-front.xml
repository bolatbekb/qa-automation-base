<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Frontend">
    <listeners>
        <listener class-name="listeners.RetryListener" />
        <listener class-name="listeners.EventListener" />
    </listeners>
    <test name="Regression for Frontend" >
        <groups>
            <run>
                <include name="calculator"/>
            </run>
        </groups>

        <packages>
            <package name="frontend.*" />
        </packages>
    </test>
</suite>